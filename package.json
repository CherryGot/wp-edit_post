{
  "name": "@cherrygot/kruti_admin",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@wordpress/stylelint-config": "^21.0.0",
    "auto-changelog": "^2.4.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.17.3",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "sass": "^1.41.1",
    "stylelint": "^14.12.0",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "lint": "stylelint src/**/*.scss && eslint src/ *.js",
    "changelog": "auto-changelog --commit-limit false",
    "watch:sass": "sass --load-path=node_modules/ --watch src/:assets/compiled/ --no-source-map",
    "build:sass": "sass --load-path=node_modules/ src/style.scss:assets/compiled/style.css --no-source-map --style=compressed",
    "watch:js": "webpack --mode=development --watch",
    "build:js": "webpack"
  }
}
