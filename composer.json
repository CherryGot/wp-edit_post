{
  "name": "cherrygot/genz-admin",
  "type": "wordpress-muplugin",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Chakrapani Gautam",
      "email": "contact@cherrygot.me"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/CherryGot/Coding-Standards.git"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "cherrygot/coding-standards": "^0.4",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "CherryGot\\GenZPress\\MustUsePlugins\\Admin\\": "src/"
    }
  },
  "scripts": {
    "lint": "phpcs --error-severity=1 --warning-severity=1",
    "test": "phpunit --colors=always",
    "generate-readme": "php ./workflow/readme-generator.php"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
