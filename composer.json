{
  "name": "cherrygot/kruti-admin",
  "description": "A Next-Gen WordPress Admin Interface.",
  "type": "project",
  "license": "GPLv3",
  "homepage": "https://cherrygot.me/projects/kruti-admin/",
  "authors": [
    {
      "name": "Chakrapani Gautam",
      "email": "contact@cherrygot.me",
      "homepage": "https://cherrygot.me/"
    }
  ],
  "keywords": [
    "composer", "wordpress", "wp", "wp-admin", "admin", "wordpress admin", "dashboard", "admin-theme", "theme"
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7.0",
    "roave/security-advisories": "dev-master",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "wp-coding-standards/wpcs": "^2.3",
    "drupal/coder": "^8.3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org/",
      "only": ["wpackagist-plugin/*"]
    }
  ],
  "autoload": {
    "psr-4": {
      "Kruti\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "lint": ["phpcs"]
  }
}
